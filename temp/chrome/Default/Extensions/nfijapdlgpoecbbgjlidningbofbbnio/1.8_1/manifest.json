{
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "content.js" ],
      "matches": [ "https://docs.google.com/spreadsheets/*" ]
   } ],
   "description": "adds plugins to Google Sheets menu bar to trigger Intern jobs",
   "host_permissions": [ "https://www.internalfb.com/", "https://*.internalfb.com/" ],
   "key": "",
   "manifest_version": 3,
   "name": "Meta Google Sheets Plugin Provider",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "declarativeNetRequest", "declarativeNetRequestFeedback", "storage" ],
   "update_url": "https://www.internalfb.com/intern/browser_extensions/chrome/update.xml",
   "version": "1.8"
}
