{
   "background": {
      "service_worker": "serviceWorker.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "contentScript.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Logs usage of allowlisted work apps",
   "host_permissions": [ "\u003Call_urls>" ],
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "92",
   "name": "Meta Work App Usage",
   "permissions": [ "storage", "background", "tabs", "scripting", "gcm", "alarms" ],
   "update_url": "https://our.intern.facebook.com/intern/browser_extensions/chrome/update.xml/",
   "version": "1.2.12"
}
