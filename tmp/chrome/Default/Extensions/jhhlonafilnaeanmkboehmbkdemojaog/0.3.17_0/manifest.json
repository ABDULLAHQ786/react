{
   "background": {
      "persistent": false,
      "scripts": [ "static/js/background.js" ]
   },
   "browser_action": {
      "default_icon": "1_1.png",
      "default_popup": "popup.html",
      "default_title": "One On One Extension"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "linux": "Alt+Shift+O",
            "mac": "Alt+Shift+O",
            "windows": "Alt+Shift+O"
         }
      }
   },
   "content_security_policy": "script-src 'self'; object-src 'self'; frame-ancestors 'self' https://fb.facebook.com https://*.internalfb.com;",
   "description": "Quickly add shared topics to discuss in your 1:1s. Uninstall any time. To improve the tool, tracks anonymously and only when used.",
   "homepage_url": "https://fb.workplace.com/groups/381959089072312/",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "43",
   "name": "1:1",
   "permissions": [ "storage", "*://*.facebook.com/", "*://*.internalfb.com/", "activeTab", "tabs", "notifications" ],
   "update_url": "https://our.intern.facebook.com/intern/browser_extensions/chrome/update.xml/",
   "version": "0.3.17"
}
