{
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content_checker.js" ],
      "matches": [ "\u003Call_urls>" ]
   }, {
      "js": [ "content_selector.js" ],
      "matches": [ "http://localhost:*/*", "https://www.facebook.com/*", "https://www.internalfb.com/login*", "https://www.internalfb.com/intern/password*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "default-src 'self'; script-src 'self'; style-src 'self'; connect-src https://graph.facebook.com https://www.internalfb.com"
   },
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "add_custom_engine",
         "path": "add_origin.json"
      } ]
   },
   "description": "Password reuse and phishing protection for Facebook employees. Made and supported by the Blackbird team.",
   "homepage_url": "https://www.internalfb.com/intern/wiki/Blackbird/Password_protection_QnA/",
   "host_permissions": [ "https://*.facebook.com/*", "https://*.internalfb.com/*", "https://*.workplace.com/*" ],
   "icons": {
      "16": "icons/Protect-the-graph.png",
      "48": "icons/Protect-the-graph.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Blackbird password protection",
   "permissions": [ "tabs", "scripting", "storage", "cookies", "alarms", "declarativeNetRequest" ],
   "short_name": "BB Pwd Prot",
   "update_url": "https://www.internalfb.com/intern/browser_extensions/chrome/update.xml/",
   "version": "1.1.9"
}
