{
   "action": {
      "default_icon": "public/icon.png",
      "default_popup": "public/popup.html"
   },
   "background": {
      "service_worker": "src/serviceWorker.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "src/redirectBanner.js", "src/popup.js" ],
      "matches": [ "*://fb.quip.com/*" ]
   } ],
   "content_security_policy": {
      "content_scripts": "script-src 'self'; object-src 'self'",
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "This extension helps you redirect Quip and Dropbox file links to Google Workspace if the files have already been migrated. It also includes a Quip2Google Assistant that can help fix issues on Google Docs that were migrated from Quip.",
   "host_permissions": [ "*://fb.quip.com/*", "*://www.dropbox.com/*", "*://*.facebook.com/", "*://*.internalfb.com/" ],
   "key": "",
   "manifest_version": 3,
   "name": "Tool Consolidation",
   "permissions": [ "activeTab", "history", "storage", "tabs", "webNavigation", "webRequest" ],
   "update_url": "https://www.internalfb.com/intern/browser_extensions/chrome/update.xml",
   "version": "1.3.6"
}
